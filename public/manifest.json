{
  "manifest_version": 3,
  "version": "1.0",
  "short_name": "React App",
  "name": "SaveMyLink",
  "action": {
    "default_popup": "index.html",
    "default_icon": "hive_logo.png"
  },
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "host_permissions": ["<all_urls>"],
  "description": "Save your links and revisit them later with easy access"
}